@model IEnumerable<RestaurantRatingsApp.Data.Models.AppUser>
 
<h2>Users</h2>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Access</a>
            </td>
        </tr>
    }
</table>